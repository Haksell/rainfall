#include <stdlib.h>
#include <string.h>

int main(int argc, char *argv[])
{
    int     n;
    char    buffer[40];

    (void) argc;
    atoi(argv[1])
    if (n > 9)
        return (1);
    memcpy(buffer, argv[2], 4 * n);
    if (n != 0x574f4c46)
        return (0);
    execl("/bin/sh", "sh", 0);
    return (0);
}
