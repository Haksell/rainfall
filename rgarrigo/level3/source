#include <stdio.h>
#include <stdlib.h>

int v(void)
{
    char    buffer[512];

    fgets(buffer, 512, stdin);
    printf(buffer);
    if (*(int *)(0x0804988c) != 0x40)
        return (*(int *)(0x0804988c));
    fwrite("Wait what?!\n", 1, 12, stdout);
    return (system("/bin/sh"));
}

int main(void)
{
    return (v());
}
