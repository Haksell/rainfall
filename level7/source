#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char    flag[68];

typedef struct s_arg
{
    int     n;
    char    *str;
}   t_arg;

void    m(void)
{
    printf("%s - %d\n", flag, time(0));
}

int main(int argc, char *argv[])
{
    t_arg *arg1;
    t_arg *arg2;

    (void) argc;
    arg1 = malloc(sizeof(t_arg));
    arg1.n = 1;
    arg1.str = malloc(8);
    arg2 = malloc(sizeof(t_arg));
    arg2.n = 2;
    arg2.str = malloc(8);
    strcpy(arg1.str, argv[1]);
    strcpy(arg2.str, argv[2]);
    fgets(flag, 68, fopen("/home/user/level8/.pass", "r"));
    puts("~~");
    return (0);
}
